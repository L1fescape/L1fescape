{
  "name": "l1fescape.com",
  "workspaceFolder": "/workspace",
  "image": "node:17",
  "runArgs": ["-it"],
  "extensions": [
    "ms-vscode.vscode-typescript-tslint-plugin",
    "esbenp.prettier-vscode",
    "msjsdiag.debugger-for-chrome",
    "eamodio.tsl-problem-matcher"
  ],
  "settings": {
    "firefox.args": []
  },
  "appPort": 3000,
  "mounts": [
    "source=${localWorkspaceFolder},target=${containerWorkspaceFolder},type=bind,consistency=cached",
    "source=try-node-node_modules,target=${containerWorkspaceFolder}/node_modules,type=volume",
    "source=try-build,target=${containerWorkspaceFolder}/build,type=volume"
  ]
}
