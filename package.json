{
  "name": "ak.gg",
  "version": "0.0.1",
  "main": "web/index.tsx",
  "license": "MIT",
  "scripts": {
    "start": "yarn && yarn dev:web",
    "build": "yarn && yarn build:web",
    "build:web": "cross-env NODE_ENV=production webpack --config web/webpack.config.js",
    "dev:web": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --config web/webpack.config.js",
    "test": "jest"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.8.1",
    "copy-webpack-plugin": "^5.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "node-sass": "^4.11.0",
    "react-test-renderer": "^16.8.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "typescript": "^3.4.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "^ak(.*)$": "<rootDir>/src$1",
      "^web(.*)$": "<rootDir>/web$1",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
