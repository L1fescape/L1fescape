{
  "name": "l1fescape.com",
  "version": "0.0.2",
  "main": "web/index.tsx",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "build": "cross-env NODE_ENV=production webpack --config web/webpack.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config web/webpack.config.js",
    "analyze": "cross-env WEBPACK_ANALYZE=true webpack --config web/webpack.config.js",
    "clean": "rimraf build/*",
    "test": "jest",
    "data": "npm run data:spotify && npm run data:lastfm",
    "data:spotify": "ts-node src/scripts/gen-playlist-data.ts",
    "data:lastfm": "ts-node src/scripts/gen-track-data.ts",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.10",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.5",
    "eslint": "^8.7.0",
    "he": "^1.2.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-player": "^2.9.0",
    "react-spring": "^8.0.27"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.1",
    "@types/he": "^1.1.1",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-test-renderer": "^16.9.5",
    "copy-webpack-plugin": "^10.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^6.5.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "mini-css-extract-plugin": "^2.5.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^3.0.0",
    "sass": "^1.49.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^5.3.3",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.9",
    "webpack": "^5.66.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "^l1(.*)$": "<rootDir>/src$1",
      "^web(.*)$": "<rootDir>/web$1",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true
  }
}
