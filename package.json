{
  "name": "l1fescape.com",
  "version": "0.0.1",
  "main": "web/index.tsx",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "build": "cross-env NODE_ENV=production webpack --config web/webpack.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --config web/webpack.config.js",
    "analyze": "cross-env NODE_ENV=analyze webpack --config web/webpack.config.js",
    "clean": "rimraf build/*",
    "test": "jest",
    "data": "npm run data:spotify && npm run data:lastfm",
    "data:spotify": "ts-node src/components/spotify/playlist/gen-playlist-data.ts",
    "data:lastfm": "ts-node src/components/lastfm/tracks/gen-track-data.ts",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.7",
    "ak.gg": "0.0.2",
    "classnames": "^2.2.6",
    "dayjs": "^1.8.35",
    "eslint": "^6.8.0",
    "node-fetch": "^2.6.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-spring": "^8.0.27"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "@types/react-test-renderer": "^16.8.1",
    "copy-webpack-plugin": "^5.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^9.0.0",
    "typescript": "^3.4.3",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "^l1(.*)$": "<rootDir>/src$1",
      "^web(.*)$": "<rootDir>/web$1",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true
  }
}
